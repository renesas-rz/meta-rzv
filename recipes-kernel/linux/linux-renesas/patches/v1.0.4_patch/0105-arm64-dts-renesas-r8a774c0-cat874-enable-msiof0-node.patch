From c2449d732a64052b0c5fb345f5f651a3f2a017db Mon Sep 17 00:00:00 2001
From: Hien Huynh <hien.huynh.px@renesas.com>
Date: Mon, 30 Mar 2020 10:45:46 +0700
Subject: [PATCH 105/197] arm64: dts: renesas: r8a774c0-cat874: enable msiof0
 node

This patch enable MSIOF0 on SoC R8A774C0 for CAT874 Board.

Signed-off-by: Huynh Thanh Hung <hung.huynh.wz@renesas.com>
Signed-off-by: Hien Huynh <hien.huynh.px@renesas.com>
---
 arch/arm64/boot/dts/renesas/r8a774c0-cat874.dts | 14 ++++++++++++++
 1 file changed, 14 insertions(+)

diff --git a/arch/arm64/boot/dts/renesas/r8a774c0-cat874.dts b/arch/arm64/boot/dts/renesas/r8a774c0-cat874.dts
index 53fd853..a6f7559 100644
--- a/arch/arm64/boot/dts/renesas/r8a774c0-cat874.dts
+++ b/arch/arm64/boot/dts/renesas/r8a774c0-cat874.dts
@@ -19,6 +19,7 @@
 		serial1 = &hscif2;
 		serial2 = &hscif3;
 		serial3 = &hscif4;
+		spi0 = &msiof0;
 	};
 
 	chosen {
@@ -434,6 +435,12 @@
 		groups = "tpu0_to0", "tpu0_to1";
 		function = "tpu0";
 	};
+
+	msiof0_pins: spi0 {
+		groups = "msiof0_clk", "msiof0_sync",
+			 "msiof0_rxd", "msiof0_txd";
+		function = "msiof0";
+	};
 };
 
 &rcar_sound {
@@ -635,3 +642,10 @@
 
 	status = "okay";
 };
+
+&msiof0 {
+	pinctrl-0 = <&msiof0_pins>;
+	pinctrl-names = "default";
+
+	status = "okay";
+};
-- 
