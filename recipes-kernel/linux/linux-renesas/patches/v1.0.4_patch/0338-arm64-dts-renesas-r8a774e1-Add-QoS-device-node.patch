From 018d80acbc21acecadb5ee2c06241b9337208401 Mon Sep 17 00:00:00 2001
From: Hien Huynh <hien.huynh.px@renesas.com>
Date: Fri, 15 May 2020 16:26:06 +0700
Subject: [PATCH 338/338] arm64: dts: renesas: r8a774e1: Add QoS device node

This patch adds QoS device node for the r8a774e1 SoC.

Signed-off-by: Hien Huynh <hien.huynh.px@renesas.com>
---
 arch/arm64/boot/dts/renesas/r8a774e1.dtsi | 6 ++++++
 1 file changed, 6 insertions(+)

diff --git a/arch/arm64/boot/dts/renesas/r8a774e1.dtsi b/arch/arm64/boot/dts/renesas/r8a774e1.dtsi
index 280c6f2..a2709f1 100644
--- a/arch/arm64/boot/dts/renesas/r8a774e1.dtsi
+++ b/arch/arm64/boot/dts/renesas/r8a774e1.dtsi
@@ -753,6 +753,12 @@
 			status = "disabled";
 		};
 
+
+		qos@e67e0000 {
+			compatible = "renesas,qos";
+			reg = <0 0xe67e0000 0 0x10090>;
+		};
+
 		cmt0: timer@e60f0000 {
 			compatible = "renesas,r8a774e1-cmt0",
 				     "renesas,rcar-gen3-cmt0";
-- 
