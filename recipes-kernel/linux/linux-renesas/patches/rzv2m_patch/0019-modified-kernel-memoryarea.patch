From e769fb986279877793d4ed72c539af440915eb43 Mon Sep 17 00:00:00 2001
From: Anh Ly <anh.ly.yb@renesas.com>
Date: Mon, 13 Sep 2021 10:31:50 +0700
Subject: [PATCH] modified-kernel-memoryarea

Signed-off-by: Anh Ly <anh.ly.yb@renesas.com>
---
 arch/arm64/boot/dts/renesas/r9a09g011gbg-evaluation-board.dts | 4 ++--
 arch/arm64/kernel/head.S                                      | 2 +-
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm64/boot/dts/renesas/r9a09g011gbg-evaluation-board.dts b/arch/arm64/boot/dts/renesas/r9a09g011gbg-evaluation-board.dts
index fb524dd..b9ea5e2 100644
--- a/arch/arm64/boot/dts/renesas/r9a09g011gbg-evaluation-board.dts
+++ b/arch/arm64/boot/dts/renesas/r9a09g011gbg-evaluation-board.dts
@@ -22,10 +22,10 @@
                stdout-path = "serial0:115200n8";
        };
 
-       memory@10000000 {
+       memory@08000000 {
                device_type = "memory";
                /* first 256MB is reserved for secure area. */
-               reg = <0x00000000 0x10000000 0x0 0x70000000>;
+               reg = <0x00000000 0x08000000 0x0 0x78000000>;
        };
 
        reserved-memory {
diff --git a/arch/arm64/kernel/head.S b/arch/arm64/kernel/head.S
index 82ef029..b73739d 100644
--- a/arch/arm64/kernel/head.S
+++ b/arch/arm64/kernel/head.S
@@ -114,7 +114,7 @@ pe_header:
 	 *  x19/x20    __primary_switch()         callee preserved temp registers
 	 */
 ENTRY(stext)
-        mov     x0, #0x10000000
+        mov     x0, #0x08000000
 //      mov     x4, #0x40007fc0
 	bl	preserve_boot_args
         bl      el3_setup                       // Drop to EL2, w0=cpu_boot_mode
-- 
2.7.4

